{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "deleted": {
      "type": "boolean"
    },
    "orderindex": {
      "type": "integer"
    },
    "priority": {
      "type": "null"
    },
    "assignee": {
      "type": "null"
    },
    "due_date": {
      "type": "null"
    },
    "start_date": {
      "type": "null"
    },
    "folder": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
